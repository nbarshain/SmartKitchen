<h1> All Lists</h1>

<table>
	<tr>
		<th>Name</th>
		<th colspan="2"></th>
	</tr>
	<% @lists.each do |list| %>
		<tr>
			<td><%= link_to list.name, list_path(list) %></td>
			<td><%= link_to 'Edit', edit_list_path(list) %></td>
			<td><%= link_to 'Delete', list_path(list), method: :delete, data: {confirm: 'Are you sure?'} %></td>
		</tr>
	<% end %>
</table>	

<br>
<%= render 'form' %>

<br>
<%= button_to 'See All Foods', foods_path, method: :get %> 
<h3><%= link_to 'See All Cabinets', cabinets_path %></h3>